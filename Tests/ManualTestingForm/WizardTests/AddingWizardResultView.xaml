<skyui:DataBoundViewControl x:Class="AddingWizardResultView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ManualTestingForm"
      xmlns:skyui="clr-namespace:SkyEditor.UI.WPF;assembly=SkyEditor.UI.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      TargetType="{x:Type local:AddingWizardResult}">
    <Grid>
        <Label Content="Result: " HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="67,14,0,0" TextWrapping="Wrap" Text="{Binding Result, Mode=OneWay}" VerticalAlignment="Top" Width="120" IsReadOnly="True"/>
        <CheckBox Content="Looks good to me!" HorizontalAlignment="Left" Margin="67,42,0,0" VerticalAlignment="Top"/>

    </Grid>
</skyui:DataBoundViewControl>
